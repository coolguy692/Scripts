ptsm = {4, 4.2, 4.4, 4.6, 4.8, 5, 5.2, 5.4}
mouse.KeyDown:connect(function(key)
    if key == "j" then
        if Debounces.CanJoke == true then
            Debounces.CanJoke = false
            u = Instance.new("Sound",char)
            u.SoundId = "http://www.roblox.com/asset/?id=261010715"
            u.Pitch = ptsm[math.random(1,#pt)]
            u.Volume = 1
            u2 = Instance.new("Sound",char)
            u2.SoundId = "http://www.roblox.com/asset/?id=261010715"
            u2.Pitch = u.Pitch
            u2.Volume = 1
            u3 = Instance.new("Sound",char)
            u3.SoundId = "http://www.roblox.com/asset/?id=261010715"
            u3.Pitch = u.Pitch
            u3.Volume = 1
            wait(.01)
            u:Play()
            u2:Play()
            u3:Play()
            wait(1.5)
            u:Destroy()
            u2:Destroy()
            u3:Destroy()
            if Debounces.CanJoke == false then
                Debounces.CanJoke = true
            end
        end
    end
end)
