SlashFireA = New("Animation",tool,"SlashFire",{AnimationId = "http://www.roblox.com/asset/?id=186934658",})
PunchA = New("Animation",tool,"Punch",{AnimationId = "http://www.roblox.com/asset?id=126749742",})
SpinA = New("Animation",tool,"Spin",{AnimationId = "http://www.roblox.com/asset/?id=235542946",})
RiseA = New("Animation",tool,"Rise",{AnimationId = "http://www.roblox.com/asset/?id=169638954",})
IdleAnim = chara.Humanoid:LoadAnimation(IdleA)
SLAnim = chara.Humanoid:LoadAnimation(SlashLeftA)
SRAnim = chara.Humanoid:LoadAnimation(SlashRightA)
AimAnim = chara.Humanoid:LoadAnimation(AimA)
ChargeSAnim = chara.Humanoid:LoadAnimation(ChargeSwordA)
ChargeUAnim = chara.Humanoid:LoadAnimation(ChargeUpA)
BlockAnim = chara.Humanoid:LoadAnimation(BlockA)
JumpUAnim = chara.Humanoid:LoadAnimation(JumpUpA)
JumpDAnim = chara.Humanoid:LoadAnimation(JumpDownA)
EquipAnim = chara.Humanoid:LoadAnimation(EquipA)
SlashFireAnim = chara.Humanoid:LoadAnimation(SlashFireA)
PunchAnim = chara.Humanoid:LoadAnimation(PunchA)
SpinAnim = chara.Humanoid:LoadAnimation(SpinA)
RiseAnim = chara.Humanoid:LoadAnimation(RiseA)

IdleAnim:Play()

--Sounds--
SlashSnd = New("Sound",tool,"SlashSnd",{SoundId = "http://www.roblox.com/asset/?id=45885030", Volume = 1})
HitSnd = New("Sound",tool,"HitSnd",{SoundId = "http://www.roblox.com/asset/?id=154965973", Volume = 1})
AimSnd = New("Sound",tool,"AimSnd",{SoundId = "http://www.roblox.com/asset/?id=300915902", Volume = 1})
ShootSnd = New("Sound",tool,"ShootSnd",{SoundId = "http://www.roblox.com/asset/?id=200633327", Volume = 1})
TeleSnd = New("Sound",tool,"TeleSnd",{SoundId = "http://www.roblox.com/asset/?id=153613030", Volume = 1})
SJumpSnd = New("Sound",tool,"SJumpSnd",{SoundId = "http://www.roblox.com/asset/?id=169445602", Volume = 1})
ExplSnd = New("Sound",tool,"ExplSnd",{SoundId = "http://www.roblox.com/asset/?id=142070127", Volume = 1})
ShieldSnd = New("Sound",tool,"ShieldSnd",{SoundId = "http://www.roblox.com/asset/?id=187933025", Volume = 1})
GongSnd = New("Sound",tool,"GongSnd",{SoundId = "http://www.roblox.com/asset/?id=96098241", Volume = 1})
BurnSnd = New("Sound",tool,"BurnSnd",{SoundId = "http://www.roblox.com/asset/?id=32791565", Volume = 3})
BurningSnd = New("Sound",tool,"BurningSnd",{SoundId = "http://www.roblox.com/asset/?id=31760113", Volume = 5, Looped = true})
BurnSlashSnd = New("Sound",tool,"BurnSlashSnd",{SoundId = "http://www.roblox.com/asset/?id=128912290", Volume = 5, PlaybackSpeed = 2.5})
BurnPunchSnd = New("Sound",tool,"BurnPunchSnd",{SoundId = "http://www.roblox.com/asset/?id=137579113", Volume = 5})
MudaSnd = New("Sound",tool,"MudaSnd",{SoundId = "http://www.roblox.com/asset/?id=616593932", Volume = 5})
EquipSnd = New("Sound",tool,"EquipSnd",{SoundId = "http://www.roblox.com/asset/?id=211059653", Volume = 1})
UnEquipSnd = New("Sound",tool,"UnEquipSnd",{SoundId = "http://www.roblox.com/asset/?id=211134014", Volume = 1})
Snd = New("Sound",tool,"Snd",{SoundId = "", Volume = 1})

Music = New("Sound",tool,"Music",{SoundId = "rbxassetid://157580176", Volume = 1, Looped = true})
Music:Play()
mus = true

--Name Tag--
local naeeym = Instance.new("BillboardGui",chara)
naeeym.Size = UDim2.new(0,100,0,40)
naeeym.StudsOffset = Vector3.new(0,2,0)
naeeym.Adornee = chara.Head
local tecks = Instance.new("TextLabel",naeeym)
tecks.BackgroundTransparency = 1
tecks.BorderSizePixel = 0
tecks.Text = "Ryuneko"
tecks.Font = "Fantasy"
tecks.FontSize = "Size24"
tecks.TextStrokeTransparency = 0
tecks.TextStrokeColor3 = Color3.new(1,1,0)
tecks.TextColor3 = Color3.new(1,0,0)
tecks.Size = UDim2.new(1,0,0.5,0)
local htecks = Instance.new("TextLabel",naeeym)
htecks.BackgroundTransparency = 1
htecks.BorderSizePixel = 0
htecks.Text = chara.Humanoid.Health.."/"..chara.Humanoid.MaxHealth
htecks.Font = "Fantasy"
htecks.FontSize = "Size24"
htecks.TextStrokeTransparency = 0
htecks.TextStrokeColor3 = Color3.new(1,1,0)
htecks.TextColor3 = Color3.new(1,0,0)
htecks.Size = UDim2.new(1,0,0.5,0)
htecks.Position = UDim2.new(0,0,.5,0)

--Chat Function--
function chatfunc(text)
chat = coroutine.wrap(function(ttt)
if chara:FindFirstChild("TalkingBillBoard")~= nil then
chara:FindFirstChild("TalkingBillBoard"):destroy()
end
local naeeym2 = Instance.new("BillboardGui",chara)
naeeym2.Size = UDim2.new(0,100,0,40)
naeeym2.StudsOffset = Vector3.new(0,3,0)
naeeym2.Adornee = chara.Head
naeeym2.Name = "TalkingBillBoard"
local tecks2 = Instance.new("TextLabel",naeeym2)
tecks2.BackgroundTransparency = 1
tecks2.BorderSizePixel = 0
tecks2.Text = text
tecks2.Font = "Fantasy"
tecks2.FontSize = "Size24"
tecks2.TextStrokeTransparency = 0
tecks2.TextColor3 = Color3.new(1,1,1)
tecks2.TextStrokeColor3 = Color3.new(0,0,0)
tecks2.Size = UDim2.new(1,0,0.5,0)
wait(1)
for i = 1, 5 do
wait(.01)
tecks2.Position = tecks2.Position - UDim2.new(0,0,.05,0)
tecks2.TextStrokeTransparency = tecks2.TextStrokeTransparency +.2
tecks2.TextTransparency = tecks2.TextTransparency + .2
end
naeeym2:Destroy()
end)
chat(text)
end

--Cloak!--
function Cloak(tr)
for i = 1, #katanapart do
katanapart[i].Transparency = tr
end
for i = 1, #sheathpart do
sheathpart[i].Transparency = tr
end
for i = 1, #lap do
lap[i].Transparency = tr
end
for i = 1, #rap do
rap[i].Transparency = tr
end
for i = 1, #llp do
llp[i].Transparency = tr
end
for i = 1, #rlp do
rlp[i].Transparency = tr
end
for i = 1, #chp do
chp[i].Transparency = tr
end
for i = 1, #hdp do
hdp[i].Transparency = tr
end
Decal.Transparency = tr
end

--Unsheath--
function UnsheathK()
for i = 1, #katanapart do
katanapart[i].Transparency = 0
end
for i = 1, #sheathpart do
if sheathpart[i].Name == "HandlePart" then
sheathpart[i].Transparency = 1
end
end
EquipAnim:Play()
EquipSnd:Play()
wait(.3)
EquipAnim:Stop()
end

--Sheath--
function SheathK()
BlockAnim:Play()
UnEquipSnd:Play()
wait(.3)
BlockAnim:Stop()
for i = 1, #katanapart do
katanapart[i].Transparency = 1
end
for i = 1, #sheathpart do
if sheathpart[i].Name == "HandlePart" then
sheathpart[i].Transparency = 0
end
end
end

SheathK()

--Fire Effect-
function FireEffect(part)
if part:FindFirstChild("FireEffect")==nil then
local fireeff = Instance.new("ParticleEmitter",part)
fireeff.Name = "FireEffect"
fireeff.Color = ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.new(1,0,0)),ColorSequenceKeypoint.new(1,Color3.new(1,1,0))})
fireeff.LightEmission = 1
fireeff.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(1,0)})
fireeff.Texture = "rbxasset://textures/particles/smoke_main.dds"
fireeff.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,1)})
fireeff.Acceleration = Vector3.new(0,30,0)
fireeff.Lifetime = NumberRange.new(.5)
fireeff.Rate = 1000
fireeff.Rotation = NumberRange.new(0,359)
fireeff.RotSpeed = NumberRange.new(100)
fireeff.Speed = NumberRange.new(0)
fireeff.VelocitySpread = 10
return fireeff
end
end
bladeeff = FireEffect(Hitbox)
bladeeff.Enabled = false

--Burn Damage--
function burndmg(dude,damage,env,toim)
burndude = coroutine.wrap(function(dude2,damage2,env2,toim2)
if dude:FindFirstChild("IsBurning")==nil and dude ~= chara then
local fira = nil
local torsy = nil
local val = Instance.new("BoolValue",dude)
val.Name = "IsBurning"
if dude:FindFirstChild("Torso")~=nil then
fira = FireEffect(dude.Torso)
torsy = dude.Torso
elseif dude:FindFirstChild("UpperTorso")~=nil then
fira = FireEffect(dude.UpperTorso)
torsy = dude.UpperTorso
end
local snd1 = BurnSnd:clone()
snd1.PlayOnRemove = true
snd1.Parent = torsy
snd1:Remove()
local snd2 = BurningSnd:clone()
snd2.Parent = torsy
snd2:Play()
for i = 1, toim2*2 do
dealdmg(dude2,damage2,env2,0)
wait(.5)
end
snd2:Destroy()
fira:Destroy()
val:Destroy()
end
end)
burndude(dude,damage,env,toim)
end

--Damage Function--
function dealdmg(dude,damage,env,toim)
hurt = coroutine.wrap(function(dude2,damage2,env2,toim2)
if dude ~= chara and dude:FindFirstChild("IsHit") == nil then
finaldmg = damage + math.random(-env,env)
dude.Humanoid.Health = dude.Humanoid.Health - finaldmg
local vall = Instance.new("ObjectValue",dude)
vall.Name = "IsHit"
debby:AddItem(vall,toim)
local naeeym2 = Instance.new("BillboardGui",dude)
naeeym2.Size = UDim2.new(0,100,0,40)
naeeym2.StudsOffset = Vector3.new(0,3,0)
naeeym2.Adornee = dude.Head
naeeym2.Name = "TalkingBillBoard"
local tecks2 = Instance.new("TextLabel",naeeym2)
tecks2.BackgroundTransparency = 1
tecks2.BorderSizePixel = 0
tecks2.Text = "-"..finaldmg
tecks2.Font = "Fantasy"
tecks2.FontSize = "Size24"
tecks2.TextStrokeTransparency = 0
tecks2.TextColor3 = Color3.new(1,0.6,0)
tecks2.TextStrokeColor3 = Color3.new(0,0,0)
tecks2.Size = UDim2.new(1,0,0.5,0)
for i = 1, 5 do
wait(.1)
tecks2.Position = tecks2.Position - UDim2.new(0,0,.05,0)
tecks2.TextStrokeTransparency = tecks2.TextStrokeTransparency +.2
tecks2.TextTransparency = tecks2.TextTransparency + .2
end
naeeym2:Destroy()
end
end)
hurt(dude,damage,env,toim)
end

--Blade Touch--
bladeactive = false
burnactive = false
function bladehit(hit)
if bladeactive == true then
if hit.Parent:FindFirstChild("Humanoid") ~= nil then
dealdmg(hit.Parent,12,2,.1)
if burnactive == true then
burndmg(hit.Parent,6,3,5)
end
HitSnd:Play()
end
end
end
Hitbox.Touched:connect(bladehit)

--Teleport--
function Teleport(pos)
if dell == false then
dell = true
TeleSnd:Play()
local trace = Instance.new("Part",game.Workspace)
trace.Size = Vector3.new(0,0,0)
trace.Position = chara.Torso.Position
trace.CFrame = chara.Torso.CFrame
trace.Anchored = true
trace.CanCollide = false
local mehs = Instance.new("SpecialMesh",trace)
mehs.Scale = Vector3.new(5,10,5)
mehs.MeshId = "http://www.roblox.com/asset/?id=1095708"
tracedisappear = coroutine.wrap(function()
for i = 1, 9 do
wait(.01)
trace.Transparency = trace.Transparency + .1
mehs.Scale = mehs.Scale + Vector3.new(.5,1,.5)
end
trace:Destroy()
end)
tracedisappear()
chara.Torso.CFrame = CFrame.new(pos.X,pos.Y,pos.Z)
dell = false
ChargeUAnim:Stop()
end
end

--Burn Slash--
function BurnSlash()
if dell == false then
dell = true
UnsheathK()
ChargeUAnim:Play()
wait(.5)
bladeeff.Enabled = true
BurningSnd:Play()
BurnSnd:Play()
wait(.5)
chatfunc("Blazing Blade!")
ChargeUAnim:Stop()
bladeactive = true
burnactive = true
SLAnim:Play()
local bssnd = BurnSlashSnd:clone()
bssnd.Parent = chara
bssnd.PlayOnRemove = true
bssnd:Destroy()
SRAnim:Stop()
wait(.3)
SRAnim:Play()
local bssnd = BurnSlashSnd:clone()
bssnd.Parent = chara
bssnd.PlayOnRemove = true
bssnd:Destroy()
SLAnim:Stop()
wait(.3)
dell = false
bladeeff.Enabled = false
bladeactive = false
burnactive = false
BurningSnd:Stop()
SRAnim:Stop()
SheathK()
end
end

--Wave of Fire--
function FireWave()
if dell == false then
dell = true
UnsheathK()
bladeeff.Enabled = true
SlashFireAnim:Play()
BurnSlashSnd:Play()
chatfunc("Fire Wave!")
wait(.2)
local firewave = Instance.new("Part",game.Workspace)
firewave.Size = Vector3.new(8,1,1)
firewave.Transparency = 1
firewave.CanCollide = false
firewave.Position = chara.HumanoidRootPart.Position + chara.HumanoidRootPart.CFrame.lookVector * 5
firewave.CFrame = chara.HumanoidRootPart.CFrame + chara.HumanoidRootPart.CFrame.lookVector * 5
FireEffect(firewave)
local bodf = Instance.new("BodyForce",firewave)
bodf.Force = Vector3.new(0,game.Workspace.Gravity*firewave:GetMass(),0)
firewave.Velocity = chara.HumanoidRootPart.CFrame.lookVector * 50
debby:AddItem(firewave,10)
firewave.Touched:connect(function(hit)
if hit.Parent ~= chara and hit.Parent:FindFirstChild("Humanoid") ~= nil then
dealdmg(hit.Parent,15,2,1)
burndmg(hit.Parent,5,2,5)
end
end)
wait(.7)
bladeeff.Enabled = false
SheathK()
dell = false
end
end

--Sonic Dice--
function SonicDice()
if dell == false then
dell = true
chatfunc("Sonic Dice...")
chara.Humanoid.WalkSpeed = 0
UnsheathK()
ChargeUAnim:Play()
wait(2)
chara.Humanoid.WalkSpeed = 50
Cloak(1)
ChargeUAnim:Stop()
SpinAnim:Play()
local slashpart = Instance.new("Part",chara)
slashpart.Transparency = 1
slashpart.CanCollide = false
slashpart.Size = Vector3.new(10,10,10)
slashpart.BottomSurface = 0
slashpart.TopSurface = 0
slashpart.Position = chara.Torso.Position
slashpart.CFrame = chara.Torso.CFrame
local pweld = Instance.new("Weld",slashpart)
pweld.Part0 = slashpart
pweld.Part1 = chara.Torso
local pemit = Instance.new("ParticleEmitter",slashpart)
pemit.LightEmission = 1
pemit.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(.5,5),NumberSequenceKeypoint.new(1,0)})
pemit.Texture = "rbxassetid://337291948"
pemit.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(.5,0),NumberSequenceKeypoint.new(1,1)})
pemit.Lifetime = NumberRange.new(.5)
pemit.Rate = 20
pemit.Rotation = NumberRange.new(0,359)
pemit.RotSpeed = NumberRange.new(500)
pemit.Speed = NumberRange.new(0)
slashpart.Touched:connect(function(hit)
if hit.Parent:FindFirstChild("Humanoid") ~= nil then
dealdmg(hit.Parent,12,2,0)
HitSnd:Play()
end
end)
for i = 1, 100 do
wait(.1)
local bssnd = SlashSnd:clone()
bssnd.Parent = chara
bssnd.PlayOnRemove = true
bssnd.PlaybackSpeed = bssnd.PlaybackSpeed + math.random(-.5,.5)
bssnd:Destroy()
end
chara.Humanoid.WalkSpeed = 0
SpinAnim:Stop()
slashpart:Destroy()
ChargeUAnim:Play()
Cloak(0)
wait(2)
ChargeUAnim:Stop()
SheathK()
chara.Humanoid.WalkSpeed = 16
dell = false
end
end

--Burning Soul--
punchactive = false
function BurningSoul()
if dell == false then
dell = true
ChargeUAnim:Play()
chara.Humanoid.WalkSpeed = 0
for i = 1, 10 do
wait(.1)
for i = 1, #sheathpart do
sheathpart[i].Transparency = sheathpart[i].Transparency + .1
end
end
chatfunc("Burning Soul!")
BurnSnd:Play()
BurningSnd:Play()
lfire = FireEffect(chara["Left Arm"])
rfire = FireEffect(chara["Right Arm"])
local jtrace = Instance.new("Part",game.Workspace)
jtrace.Size = Vector3.new(0,0,0)
jtrace.Position = chara.Torso.Position
jtrace.CFrame = chara.Torso.CFrame - Vector3.new(0,2,0)
jtrace.Anchored = true
jtrace.BrickColor = BrickColor.new("New Yeller")
GongSnd:Play()
local tmesh = Instance.new("SpecialMesh",jtrace)
tmesh.MeshId = "http://www.roblox.com/asset/?id=1125478"
tmesh.Scale = Vector3.new(0,0,0)
for i = 1, 10 do
wait(.01)
jtrace.Transparency = jtrace.Transparency + .1
tmesh.Scale = tmesh.Scale + Vector3.new(2,.05,2)
end
jtrace:Destroy()
wait(.5)
chatfunc("All your efforts are...")
wait(.5)
chara.Humanoid.WalkSpeed = 16
ChargeUAnim:Stop()
MudaSnd:Play()
chatfunc("USELESS USELESS USELESS!")
punchactive = true
for i = 1, 4 do
PunchAnim:Play()
for i = 1, 10 do
wait(.1)
local bssnd = BurnSlashSnd:clone()
bssnd.Parent = chara
bssnd.PlayOnRemove = true
bssnd.PlaybackSpeed = bssnd.PlaybackSpeed + math.random(-.2,.2)
bssnd:Destroy()
end
end
rfire:Destroy()
lfire:Destroy()
BurningSnd:Stop()
punchactive = false
PunchAnim:Stop()
ChargeUAnim:Play()
chara.Humanoid.WalkSpeed = 0
for i = 1, 10 do
wait(.1)
for i = 1, #sheathpart do
sheathpart[i].Transparency = sheathpart[i].Transparency - .1
end
end
ChargeUAnim:Stop()
chatfunc("Give me a break...")
chara.Humanoid.WalkSpeed = 16
dell = false
end
end
function HandTouch(hit)
if punchactive == true then
if hit.Parent:FindFirstChild("Humanoid") ~= nil and hit.Parent ~= chara and hit.Parent:FindFirstChild("IsHit")==nil then
BurnPunchSnd:Play()
dealdmg(hit.Parent,5,2,0.1)
burndmg(hit.Parent,1,0,7)
end
end
end
chara["Left Arm"].Touched:connect(HandTouch)
chara["Right Arm"].Touched:connect(HandTouch)

--Dragon Wrath--
function DragonWrath()
if dell == false then
dell = true
GongSnd:Play()
local spellcircle = Instance.new("Part",chara)
spellcircle.Anchored = true
spellcircle.Size = Vector3.new(50,1,50)
spellcircle.CFrame = chara.Torso.CFrame - Vector3.new(0,2.4,0)
spellcircle.Transparency = 1
spellcircle.CanCollide = false
local blkm = Instance.new("BlockMesh",spellcircle)
blkm.Scale = Vector3.new(0,1,0)
local dec = Instance.new("Decal",spellcircle)
dec.Texture = "http://www.roblox.com/asset/?id=57955374"
dec.Transparency = 1
dec.Face = "Top"
local ptl = Instance.new("PointLight",spellcircle)
ptl.Range = 0
ptl.Color = Color3.new(1,.5,0)
local bpos = Instance.new("BodyPosition",chara.Torso)
bpos.Position = chara.Torso.Position + Vector3.new(0,10,0)
bpos.maxForce = Vector3.new(10000,10000,10000)
RiseAnim:Play()
spellc = coroutine.wrap(function()
while spellc ~= nil do
wait(.01)
spellcircle.CFrame = spellcircle.CFrame * CFrame.fromEulerAnglesXYZ(0,10,0)
end
end)
spellc()
for i = 1, 10 do
wait(.01)
ptl.Range = ptl.Range + 10
dec.Transparency = dec.Transparency - .1
blkm.Scale = blkm.Scale + Vector3.new(.2,0,.2)
end
wait(.9)
chatfunc("Are you ready to die?")
wait(1)
chatfunc("BURN IN HELL.")
wait(1)
local ex = Instance.new("Explosion",game.Workspace)
ex.Position = chara.Torso.Position
ex.BlastPressure = 0
ex.BlastRadius = 50
ex.Visible = false
ex.Hit:connect(function(hit)
if hit.Parent ~= chara and hit.Parent:FindFirstChild("Humanoid")~=nil then
local torsy = nil
if hit.Parent:FindFirstChild("Torso")~=nil then
torsy = hit.Parent.Torso
elseif hit.Parent:FindFirstChild("UpperTorso")~=nil then
torsy = hit.Parent.UpperTorso
end
local bodf = Instance.new("BodyVelocity",torsy)
bodf.Velocity = (torsy.CFrame.lookVector*-100)+Vector3.new(0,50,0)
debby:AddItem(bodf,1)
local fir = FireEffect(torsy)
fir.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,3),NumberSequenceKeypoint.new(1,0)})
fir.Acceleration = Vector3.new(0,90,0)
dealdmg(hit.Parent,50,0,1)
burndmg(hit.Parent,10,2,5)
end
end)
local fieff = FireEffect(spellcircle)
fieff.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,20),NumberSequenceKeypoint.new(1,0)})
fieff.Acceleration = Vector3.new(0,400,0)
fieff.Rate = 99999
BurningSnd:Play()
BurnSnd:Play()
wait(5)
BurningSnd:Stop()
fieff.Enabled = false
wait(1)
fieff:Destroy()
for i = 1, 10 do
wait(.01)
ptl.Range = ptl.Range - 10
dec.Transparency = dec.Transparency + .1
blkm.Scale = blkm.Scale - Vector3.new(.2,0,.2)
end
spellcircle:Destroy()
bpos:Destroy()
RiseAnim:Stop()
dell = false
end
end

--Nightmare--
didgrab = false
activegrab = false
function Nightmare(hit)
if activegrab == true then
if hit.Parent ~= chara and hit.Parent:FindFirstChild("Humanoid")~=nil then
activegrab = false
chara.Humanoid.WalkSpeed = 0
UnsheathK()
dell = true
didgrab = true
local torsy = nil
if hit.Parent:FindFirstChild("Torso")~=nil then
torsy = hit.Parent.Torso
elseif hit.Parent:FindFirstChild("UpperTorso")~=nil then
torsy = hit.Parent.UpperTorso
end
chara.Torso.CFrame = torsy.CFrame + (torsy.CFrame.lookVector*-30)
hit.Parent.Humanoid.PlatformStand = true
local bpos = Instance.new("BodyPosition",torsy)
bpos.Position = torsy.Position
bpos.maxForce = Vector3.new(10000,10000,10000)
wait(1)
local slashpart = Instance.new("Part",chara)
slashpart.Transparency = 1
slashpart.CanCollide = false
slashpart.Anchored = true
slashpart.Size = Vector3.new(10,10,10)
slashpart.BottomSurface = 0
slashpart.TopSurface = 0
slashpart.Position = torsy.Position
slashpart.CFrame = torsy.CFrame
local pemit = Instance.new("ParticleEmitter",slashpart)
pemit.LightEmission = 1
pemit.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(.5,5),NumberSequenceKeypoint.new(1,0)})
