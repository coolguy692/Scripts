tmesh2.Scale = tmesh2.Scale + Vector3.new(1,.05,1)
end
jtrace:Destroy()
jtrace2:Destroy()
end)
tracegrow()
wait(.5)
SDAnim:Stop()
combo = 1
end
end
wait(.1)
tool.Enabled = true
end

--Keys, Equip and Unequip--
function onKeyDown(key)
if spellactive == false then
if key == "z" then
CranberryTrap()
elseif key == "x" then
Lavaetein()
elseif key == "c" then
Kagome()
elseif key == "v" then
StarbowBreak()
elseif key == "b" then
Catadioptric()
elseif key == "n" then
CounterClock()
end
end
if key == "m" then
if music == 0 then
Music:Play()
music = 1
elseif music == 1 then
Music:Stop()
music = 0
end
end
end

function onEquipped(mouse)
local mouse = player:GetMouse()
if mouse then mouse.KeyDown:connect(onKeyDown)
mouse.Button1Down:connect(Click)
elseif not mouse then
end
end

function onUnequipped()
mouse = nil
end

tool.Equipped:connect(onEquipped)
tool.Unequipped:connect(onUnequipped)

