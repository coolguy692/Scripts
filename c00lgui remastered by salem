           --Main Variables
local coreGui = game:GetService("CoreGui")
--local coreGui = game:GetService("Players").LocalPlayer.PlayerGui
local plr = game:GetService("Players")
local lplr = plr.LocalPlayer

math.randomseed(tick())

--Get Player Function
function getPlrList(name)
    if name ~= nil and name ~= "" then
        local allPlrs = plr:GetPlayers()
        local lName = string.lower(name)
        local tab = {}
        if lName == "me" then table.insert(tab, lplr) elseif lName == "others" then for _,plr in pairs(allPlrs) do if plr ~= lplr then table.insert(tab, plr) end end
        elseif lName == "all" then for _,plr in pairs(allPlrs) do table.insert(tab, plr) end
        elseif lName == "guests" then for _,plr in pairs(allPlrs) do if plr.Guest then table.insert(tab, plr) end end
        elseif lName == "nonguests" then for _,plr in pairs(allPlrs) do if not plr.Guest then table.insert(tab, plr) end end
        elseif lName == "random" then table.insert(tab, allPlrs[math.random(1, #allPlrs)])
        elseif lName == "friends" then for _,plr in pairs(allPlrs) do if plr:IsFriendsWith(plr.UserId) then table.insert(tab, plr) end end
        elseif lName == "nonfriends" then for _,plr in pairs(allPlrs) do if not plr:IsFriendsWith(plr.UserId) then table.insert(tab, plr) end end
        elseif lName == "nbcs" then if plr.MembershipType == Enum.MembershipType.None then table.insert(tab, plr) end
        elseif lName == "bcs" then if plr.MembershipType == Enum.MembershipType.BuildersClub then table.insert(tab, plr) end
        elseif lName == "tbcs" then if plr.MembershipType == Enum.MembershipType.TurboBuildersClub then table.insert(tab, plr) end
        elseif lName == "nbcs" then if plr.MembershipType == Enum.MembershipType.OutrageousBuildersClub then table.insert(tab, plr) end
        elseif lName == "team" then if plr.Team == lplr.Team then table.insert(tab, plr) end
        else for _,plr in pairs(allPlrs) do if string.match(string.lower(plr.Name), lName) then table.insert(tab, plr) end end end
        return tab
    else
        return {}
    end
end

