	pcall(function()
		Char.LeftWing:destroy()
		Char.ReaperShadowHead:destroy()
	end)


	local Music = Sound(Torso,MusicID,1,3,true,false,true)
	Music.Name = 'Music'

	--// Stop animations \\--
	for _,v in next, Hum:GetPlayingAnimationTracks() do
		v:Stop();
