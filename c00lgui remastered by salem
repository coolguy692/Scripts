for i, v in pairs(m:GetChildren()) do
    if v:IsA("Part") then
        v.BrickColor = BrickColor.new("Really black")
        v.Material = "Glass"
    end
end
for i, v in pairs(m2:GetChildren()) do
    if v:IsA("Part") then
        v.BrickColor = BrickColor.new("Crimson")
        v.Material = "Granite"
    end
end
