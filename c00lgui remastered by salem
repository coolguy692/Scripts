mouse.KeyUp:connect(function(key)
    if key == "r" then
        if Charging == true then
            Charging = false
                pt:Destroy()
                pt2:Destroy()
                bl:Destroy()
                particle:Destroy()
            if Debounces.CanAttack == false then
                Debounces.CanAttack = true
                Debounces.NoIdl = false
                Debounces.on = false
            end
        end
         b:Stop()
    end
end)
