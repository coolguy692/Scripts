dec.Transparency = 1
dec.Face = "Top"
local ptl = Instance.new("PointLight",spellcircle)
ptl.Range = 0
ptl.Color = Color3.new(1,.5,0)
GongSnd:Play()
spellc = coroutine.wrap(function()
while spellc ~= nil do
wait(.01)
spellcircle.CFrame = spellcircle.CFrame * CFrame.fromEulerAnglesXYZ(0,10,0)
end
end)
spellc()
for i = 1, 10 do
wait(.01)
ptl.Range = ptl.Range + 4
dec.Transparency = dec.Transparency - .1
blkm.Scale = blkm.Scale + Vector3.new(.4,0,.4)
end
wait(1)
local ex = Instance.new("Explosion",game.Workspace)
ex.Position = chara.Torso.Position
ex.BlastPressure = 0
ex.BlastRadius = 50
ex.Visible = false
ex.Hit:connect(function(hit)
if hit.Parent ~= chara and hit.Parent:FindFirstChild("Humanoid")~=nil then
local torsy = nil
if hit.Parent:FindFirstChild("Torso")~=nil then
torsy = hit.Parent.Torso
elseif hit.Parent:FindFirstChild("UpperTorso")~=nil then
torsy = hit.Parent.UpperTorso
end
local bodf = Instance.new("BodyVelocity",torsy)
bodf.Velocity = Vector3.new(math.random(-100,100),40,math.random(-100,100))
debby:AddItem(bodf,1)
bpos:Destroy()
local fir = FireEffect(torsy)
fir.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,3),NumberSequenceKeypoint.new(1,0)})
fir.Acceleration = Vector3.new(0,90,0)
dealdmg(hit.Parent,30,0,1)
burndmg(hit.Parent,20,5,999)
end
end)
local fieff = FireEffect(spellcircle)
fieff.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,20),NumberSequenceKeypoint.new(1,0)})
fieff.Acceleration = Vector3.new(0,400,0)
fieff.Rate = 99999
BurningSnd:Play()
BurnSnd:Play()
wait(5)
BurningSnd:Stop()
fieff.Enabled = false
wait(1)
fieff:Destroy()
for i = 1, 10 do
wait(.01)
ptl.Range = ptl.Range - 4
dec.Transparency = dec.Transparency + .1
blkm.Scale = blkm.Scale - Vector3.new(.4,0,.4)
end
spellcircle:Destroy()
dell = false
chara.Humanoid.WalkSpeed = 16
didgrab = false
ChargeUAnim:Stop()
SheathK()
end
end
end
chara.Torso.Touched:connect(Nightmare)

--Button1Down--
dell = false
function onButton1Down()
if dell == false then
dell = true
UnsheathK()
bladeactive = true
for i = 1, 2 do
SLAnim:Play()
SlashSnd:Play()
SRAnim:Stop()
wait(.3)
SRAnim:Play()
SlashSnd:Play()
SLAnim:Stop()
wait(.3)
end
SheathK()
bladeactive = false
dell = false
SRAnim:Stop()
SLAnim:Stop()
end
end

--KeyDowns--
function onKeyDown(key)
if key == "z" then
Teleport(Mouse.Hit.p + Vector3.new(0,2,0))
elseif key == "x" then
BurnSlash()
elseif key == "c" then
FireWave()
elseif key == "v" then
SonicDice()
elseif key == "b" then
BurningSoul()
elseif key == "n" then
DragonWrath()
elseif key == "m" then
dell = true
chara.Humanoid.WalkSpeed = 5
ChargeUAnim:Play()
activegrab = true
wait(5)
if didgrab == false then
chatfunc("Crap.")
ChargeUAnim:Stop()
activegrab = false
dell = false
chara.Humanoid.WalkSpeed = 16
elseif didgrab == true then
end
elseif key == "l" then
if mus == true then
Music:Stop()
mus = false
elseif mus == false then
Music:Play()
mus = true
end
end
end

