AccessUI.Vertical_11.Parent = AccessUI.RoundedBG_11
AccessUI.Vertical_11.BackgroundColor3 = Color3.fromRGB(152, 255, 152)
AccessUI.Vertical_11.BorderSizePixel = 0
AccessUI.Vertical_11.Position = UDim2.new(0, 10, 0, 0)
AccessUI.Vertical_11.Size = UDim2.new(1, -20, 1, 0)
AccessUI.Vertical_11.ZIndex = 3

