for i = 1, #ch do
if ch[i].ClassName == "Part" then
ch[i].Transparency = 1
if ch[i].Name == "Torso" then
ch[i].roblox.Transparency = 1
end
if ch[i].Name == "Head" then
ch[i].face.Transparency = 1
end
elseif ch[i].ClassName == "Accessory" then
ch[i]:Destroy()
end
end

