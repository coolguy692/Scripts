	dominusghost.Name = "dominusghost"
	dominusghost.Parent = Frame
	dominusghost.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	dominusghost.BorderColor3 = Color3.fromRGB(255, 0, 0)
	dominusghost.BorderSizePixel = 4
	dominusghost.Position = UDim2.new(0.488897592, 0, 0.467680603, 0)
	dominusghost.Size = UDim2.new(0, 102, 0, 32)
	dominusghost.Font = Enum.Font.SourceSans
	dominusghost.Text = "Dominus Ghost"
	dominusghost.TextColor3 = Color3.fromRGB(255, 255, 255)
	dominusghost.TextSize = 14.000
	dominusghost.MouseButton1Click:connect(function()
		function nob(who,tra,hat)
			c=who.Character
			pcall(function()u=c["Body Colors"]
				u.HeadColor=BrickColor.new("Black")
				u.LeftLegColor=BrickColor.new("Black")
				u.RightLegolor=BrickColor.new("Black")
				u.LeftArmColor=BrickColor.new("Black")
				u.TorsoColor=BrickColor.new("Black")
				u.RightArmColor=BrickColor.new("Black")
			end)
			pcall(function()c.Shirt:Destroy() c.Pants:Destroy() end)
			for i,v in pairs(c:GetChildren()) do
				if v:IsA("BasePart") then
					v.Transparency=tra
					if v.Name=="HumanoidRootPart" or v.Name=="Head" then
						v.Transparency=1
					end
					wait()
					v.BrickColor=BrickColor.new("Black")
				elseif v:IsA("Hat") then
					v:Destroy()
				end
			end
			xx=game:service("InsertService"):LoadAsset(hat)
			xy=game:service("InsertService"):LoadAsset(47433)["LinkedSword"]
			xy.Parent=who.Backpack
			for a,hat in pairs(xx:children()) do
				hat.Parent=c
			end
			xx:Destroy()
			h=who.Character.Humanoid
			h.MaxHealth=50000
			wait(1.5)
			h.Health=50000
			h.WalkSpeed=32
		end
		nob(game.Players.LocalPlayer,0.6,21070012)
	end)

	discocharacter.Name = "discocharacter"
	discocharacter.Parent = Frame
	discocharacter.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	discocharacter.BorderColor3 = Color3.fromRGB(255, 0, 0)
	discocharacter.BorderSizePixel = 4
	discocharacter.Position = UDim2.new(0.317872673, 0, 0.589353621, 0)
	discocharacter.Size = UDim2.new(0, 112, 0, 32)
	discocharacter.Font = Enum.Font.SourceSans
	discocharacter.Text = "Disco Character"
	discocharacter.TextColor3 = Color3.fromRGB(255, 255, 255)
	discocharacter.TextSize = 14.000
	discocharacter.MouseButton1Click:connect(function()
		presets = {"Bright red","Bright yellow","Bright orange","Bright violet","Bright blue","Bright bluish green","Bright green"}
		while true do
			wait(0.5)
			ye = game.Workspace[game.Players.LocalPlayer.Name]:GetChildren()
			for i,v in pairs(ye) do
				if v.className == "Part" then
					v.BrickColor = BrickColor.new(presets[math.random(1,#presets)])
				end
			end
		end
	end)

	meshdisco.Name = "meshdisco"
	meshdisco.Parent = Frame
