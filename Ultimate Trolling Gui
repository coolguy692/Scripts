			coroutine.wrap(function()
				if firsttime5 then return end
				firsttime5 = true
				while true do
					swait()
					for i,v in pairs(Torso:GetChildren()) do
						if v.Name == "ballzor" then
							magicccc = Instance.new("Part",RightArm)
							magicccc.Material = "Neon"
