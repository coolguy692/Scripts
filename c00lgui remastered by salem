    button.MouseButton1Down:connect(function()
                       --TITLE: Change All Parts Rainbowy
local SPEED = 0.2 --You can change the speed here--
wait(2)
local Credits = Instance.new('Message', game.Workspace)
Credits.Text = "Script made by L337H3X0R"
wait(5)
Credits:Destroy()
while true do
wait(SPEED)
for i,v in pairs(game.Workspace:GetChildren()) do
if v:IsA("Part") then
v.BrickColor = BrickColor.random()
end
end
end
    end)
