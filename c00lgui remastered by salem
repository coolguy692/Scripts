     za.Scale = vt(5,5,5)
for i = 0, 49 do
ba.CFrame = barrelhole.CFrame
ba.Transparency = ba.Transparency - 0.1
za.Scale = za.Scale - vt(0.1,0.1,0.1)
wait()
end 
