AccessUI.Corner_42.Parent = AccessUI.RoundedBG_11
AccessUI.Corner_42.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AccessUI.Corner_42.BackgroundTransparency = 1.000
AccessUI.Corner_42.BorderSizePixel = 0
AccessUI.Corner_42.Position = UDim2.new(1, -10, 0, 0)
AccessUI.Corner_42.Size = UDim2.new(0, 10, 0, 10)
