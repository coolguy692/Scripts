  sa1.Pitch = 0.75
  sa1.Volume = 1
  sa1.Looped = false
  sa1:Play()
  local sans1 = Instance.new("Sound",hed)
  sans1.SoundId = "rbxassetid://200633529"
  sans1.Pitch = 0.725
  sans1.Volume = 1
  sans1.Looped = false
  sans1:Play()
