                pt:Destroy()
                pt2:Destroy()
                bl:Destroy()
            if Debounces.CanAttack == false then
                Debounces.CanAttack = true
                Debounces.NoIdl = false
                Debounces.on = false
                Debounces.grab = false

            end
        end
    end
end)

