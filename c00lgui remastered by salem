    if Selected.Value == false then
        Clicksound:Play()
        workspace[PlayerName.Text].Humanoid.Sit = true
    elseif Selected.Value == true then
        for i,v in pairs(game.Players:GetChildren()) do
            v.Character.Humanoid.Sit = true
        end
    end
end)


