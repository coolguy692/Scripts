AccessUI.DancingNoob.Name = "Dancing Noob"
AccessUI.DancingNoob.Parent = AccessUI.Page1
AccessUI.DancingNoob.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AccessUI.DancingNoob.BackgroundTransparency = 1.000
