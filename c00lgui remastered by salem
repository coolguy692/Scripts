    indicator.Text = "Current Size: "..tostring(csize)
end)

MinusOne.MouseButton1Down:connect(function()
    csize = csize - .1
    giant(plr, csize)
    indicator.Text = "Current Size: "..tostring(csize)
end)
end)                    
        end)
